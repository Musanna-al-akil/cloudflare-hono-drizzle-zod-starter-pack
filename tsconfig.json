{
  "compilerOptions": {
    "target": "ESNext",
    "jsxImportSource": "hono/jsx",
    "lib": [
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": [
      "@cloudflare/workers-types/2023-07-01",
      "vitest"
    ],
    "strict": true,
    "skipLibCheck": true
  }
}
